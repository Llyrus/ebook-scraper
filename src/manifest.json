{
  "name": "Ebook Scraper",
  "version": "1.1.1",
  "description": "Save Ebook pages as a PDF",
  "manifest_version": 3,
  "background": {
    "service_worker": "js/eventPage.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png"
    }
  },
  "icons": {
    "16": "icon.png"
  },
  "permissions": [
    "tabs",
    "declarativeContent",
    "storage",
    "unlimitedStorage",
    "downloads",
    "webRequest"
  ],
  "host_permissions": []
}